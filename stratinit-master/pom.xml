    <artifactId>stratinit-master</artifactId>
    <packaging>pom</packaging>
    <name>Strategic Initiative Master</name>
    <version>1.2.1-SNAPSHOT</version>
    <developers>
        <developer>
            <id>khstevens</id>
            <name>Ken Stevens</name>
            <email>ken.stevens@sympatico.ca</email>
            <url>www.strategicinitiative.org</url>
            <organization>Toccata Games</organization>
            <organizationUrl>www.strategicinitiative.org</organizationUrl>
            <timezone>EST</timezone>
        </developer>
    </developers>

    <organization>
        <name>Ken Stevens</name>
        <url>http://www.strategicinitiative.org/</url>
    </organization>
    <modules>
        <module>stratinit-graph</module>
        <module>stratinit-core</module>
        <module>stratinit-dao</module>
        <module>stratinit-test</module>
        <module>stratinit-server</module>
        <module>stratinit-wicket</module>
        <module>stratinit-rest</module>
    </modules>
    <properties>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <stratinit.version>1.2.1-SNAPSHOT</stratinit.version>
        <stratinit.client.version>1.3</stratinit.client.version>
        <spring.boot.version>2.3.9.RELEASE</spring.boot.version>
        <spring.core.version>5.2.13.RELEASE</spring.core.version>
        <spring.security.version>5.3.9.RELEASE</spring.security.version>
        <spring.data>2.3.6.RELEASE</spring.data>
        <wicket.version>9.2.0</wicket.version>
        <hibernate.version>5.4.27.Final</hibernate.version>
        <querydsl.version>4.4.0</querydsl.version>
        <swt.version>3.5.2</swt.version>
        <!-- flyway 7.5.3 doesn't work with boot 2.3.8 -->
        <flyway.version>6.5.7</flyway.version>
        <jackson.version>2.12.1</jackson.version>
    </properties>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-dependency-plugin</artifactId>
                    <version>3.1.2</version>
                </plugin>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>versions-maven-plugin</artifactId>
                    <version>2.8.1</version>
                    <configuration>
                        <generateBackupPoms>false</generateBackupPoms>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>3.8.1</version>
                    <configuration>
                        <release>11</release>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-surefire-plugin</artifactId>
                    <version>3.0.0-M5</version>
                    <configuration>
                        <argLine>-Xmx2048m --illegal-access=permit</argLine>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-failsafe-plugin</artifactId>
                    <version>3.0.0-M5</version>
                    <configuration>
                        <argLine>-Xmx2048m --illegal-access=permit</argLine>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
